	file = {arXiv Fulltext PDF:/home/domvwt/snap/zotero-snap/common/Zotero/storage/SPLEP99S/Wang et al. - 2019 - A Semi-supervised Graph Attentive Network for Fina.pdf:application/pdf;arXiv.org Snapshot:/home/domvwt/snap/zotero-snap/common/Zotero/storage/N8XPNTXS/2003.html:text/html},

	note = {arXiv:1908.03849 [cs, stat]
type: article},
	keywords = {Computer Science - Machine Learning, Statistics - Machine Learning},
	annote = {Comment: 7 pages, in proceedings of the 28th ACM International Conference on Information and Knowledge Management (CIKM)},
	file = {arXiv Fulltext PDF:/home/domvwt/snap/zotero-snap/common/Zotero/storage/5UGZ37CL/Li et al. - 2019 - SpecAE Spectral AutoEncoder for Anomaly Detection.pdf:application/pdf;arXiv.org Snapshot:/home/domvwt/snap/zotero-snap/common/Zotero/storage/3Q9LS239/1908.html:text/html},
